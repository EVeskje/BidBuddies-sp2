{
  "name": "auction-test",
  "version": "1.0.0",
  "description": "An auction site named BidBuddies",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist/css",
