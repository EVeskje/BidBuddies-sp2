{
  "name": "bidbuddies-sp2",
  "version": "1.0.0",
  "description": "An auction site named BidBuddies",
  "main": "index.js",
  "dependencies": {
    "bootstrap": "^5.3.3",
    "live-server": "^1.2.2"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist/css",
    "build": "npm-run-all clean build-css",
    "build-css": "sass src/scss:dist/css",
    "watch": "npm-run-all --parallel watch-css start-server",
    "watch-css": "sass --watch src/scss:dist/css",
    "start-server": "live-server",
    "start": "npm-run-all build start-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EVeskje/BidBuddies-sp2.git"
  },
  "keywords": [
    "auction",
    "site",
    "bootstrap",
    "sass"
  ],
